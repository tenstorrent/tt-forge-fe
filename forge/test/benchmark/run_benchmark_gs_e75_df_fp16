# ------------------------------------------------------- #
# Grayskull e75, two-row harvested chip, grid size: 8x12
# ------------------------------------------------------- #

export FORGE_FORCE_EMULATE_HARVESTED=2

# Vit
forge/test/benchmark/benchmark.py -m vit -c base -df Fp16_b -mf HiFi2 -o forge-silicon-gs-e75-perf-all-perf.json

# Deit
forge/test/benchmark/benchmark.py -m deit -c base -df Fp16_b -mf HiFi2 -o forge-silicon-gs-e75-perf-all-perf.json

# Hrnet
forge/test/benchmark/benchmark.py -m hrnet -c v2_w64 -df Fp16_b -mf HiFi3 -o forge-silicon-gs-e75-perf-all-perf.json

# Bert
forge/test/benchmark/benchmark.py -m bert -c large_tc -df Fp16_b -mf HiFi3 -o forge-silicon-gs-e75-perf-all-perf.json